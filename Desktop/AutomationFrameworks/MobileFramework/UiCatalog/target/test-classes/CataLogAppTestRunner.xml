<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="iOS test suite on iPhone Simulator">

    <parameter name="OS" value="ios"/>
    <parameter name="platformType" value="iPhone"/>
    <parameter name="appType" value="Native"/>
    <parameter name="deviceType" value="Simulator"/>
    <parameter name="deviceName" value="iPhone XS Max"/>
    <parameter name="version" value="12.1"/>

    <test name="Go to Home page" preserve-order="true">
        <classes>
            <class name="iPhone.TextFieldsTest"/>
            <class name="iPhone.PickerTest"/>
            <class name="iPhone.AlertsTest"/>
            <class name="iPhone.ControlTest"/>
            <class name="iPhone.SegmentTest"/>
            <class name="iPhone.ImageTest"/>
        </classes>
    </test>
</suite>
